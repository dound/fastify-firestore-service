{
    "[javascript]": {
        "editor.formatOnSave": false,
        "editor.tabSize": 2
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "editor.rulers": [
        79
    ],
    "eslint.validate": [
        "javascript"
    ],
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "standard.autoFixOnSave": true,
    "standard.usePackageJson": true,
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": ".*",
                "cmd": "embed-markdown"
            }
        ]
    },
    "cSpell.words": [
        "brotli",
        "concat",
        "dound",
        "fastify",
        "firestore",
        "leaderboard",
        "pino",
        "redir",
        "registrator",
        "retryable",
        "traceback",
        "unmocked",
        "UNVALIDATED",
        "uuidv"
    ]
}
